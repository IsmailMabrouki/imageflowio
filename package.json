{
  "name": "imageflowio",
  "version": "0.0.4",
  "description": "A configurable image preprocessing and inference pipeline library",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "imageflowio": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --dts --format cjs,esm --sourcemap --out-dir dist --clean",
    "dev": "tsc -w",
    "test": "npm run build && vitest run --reporter=dot",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "image",
    "pipeline",
    "preprocessing",
    "inference",
    "tensorflow",
    "typescript"
  ],
  "author": "Ismail Mabrouki",
  "license": "MIT",
  "type": "commonjs",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.3",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "sharp": "^0.33.3"
  },
  "peerDependencies": {
    "onnxruntime-node": "*"
  },
  "peerDependenciesMeta": {
    "onnxruntime-node": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "docs",
    "config.schema.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsmailMabrouki/imageflowio.git"
  },
  "bugs": {
    "url": "https://github.com/IsmailMabrouki/imageflowio/issues"
  },
  "homepage": "https://github.com/IsmailMabrouki/imageflowio#readme"
}
