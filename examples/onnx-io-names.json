{
  "$schema": "../config.schema.json",
  "model": {
    "path": "./examples/models/unet.onnx",
    "inputName": "input_0",
    "outputName": "output_0"
  },
  "input": { "type": "image", "source": "./examples/images/sample.png" },
  "preprocessing": {
    "format": { "dataType": "float32", "channels": 3, "channelOrder": "rgb" },
    "normalize": {
      "apply": true,
      "mean": [0.5, 0.5, 0.5],
      "std": [0.5, 0.5, 0.5]
    }
  },
  "postprocessing": { "denormalize": { "apply": true, "scale": 255 } },
  "output": {
    "save": { "apply": true, "path": "./examples/outputs", "format": "png" }
  }
}
